        //     "after": [],
        //     "commands": ["workbench.action.files.revert"]
        // }
    ],

    "workbench.iconTheme": "simple-icons",
    "workbench.colorTheme": "Plastic",
    "workbench.activityBar.visible": false,
    "window.enableMenuBarMnemonics": false,
    "window.titleBarStyle": "custom",
}